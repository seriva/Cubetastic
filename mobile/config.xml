<?xml version='1.0' encoding='utf-8'?>
<widget id="com.qdfpsjs" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>QDFPSJS</name>
    <description>
    </description>
    <author email="luukvanvenrooij84@gmail.com" href="http://www.luukvanvenrooij.nl">
        Luuk van Venrooij
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="resources/icon.png" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="KeepRunning" value="true" />
    <preference name="backgroundColor" value="0x00000000" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.1.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.1" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="^2.2.0" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1"/>
    <plugin name="cordova-fs-plugin-fullscreen" spec="^1.0.2" />
    <preference name="Fullscreen" value="true" />
    <preference name="fullscreen-immersive" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
