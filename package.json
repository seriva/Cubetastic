{
  "name": "genesisdevicejs",
  "version": "0.0.1",
  "description": "Genesis Device JS",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "start": "npm run build && parallelshell \"npm run server\" \"npm-watch\"",
    "postinstall": "npm run build && npm run cordova -- prepare && copyfiles \"app/xwalk-command-line\" \"app/platforms/android/assets/\" && npm run nw install 0.19.5",
    "server": "cd dist && reload -b -v",
    "build": "babel -d build src && webpack && copyfiles -u 1 \"dist/**/*.*\" \"app/www\"",
    "cordova": "cd app && cordova",
    "nw": "cd desktop && nw",
    "lint": "eslint src",
    "desktop": "npm run nw .",
    "android": "npm run build && npm run cordova -- run android",
    "android-debug": "npm run build && npm run cordova -- build android --debug"
  },
  "author": "Luuk van Venrooij",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "npm-watch": "^0.1.6",
    "parallelshell": "^2.0.0",
    "reload": "^1.0.2",
    "webpack": "^1.14.0",
    "cordova": "^6.4.0",
    "copyfiles": "^1.0.0",
    "nwjs": "^1.4.3"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "gl-matrix": "^2.3.2",
    "webgl-obj-loader": "^0.1.1"
  }
}
